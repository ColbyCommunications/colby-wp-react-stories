{
  "name": "colbycomms/wp-react-stories",
  "description":
    "A React-based, masonry-like display of categorized posts drawn from WordPress REST endpoints.",
  "type": "library",
  "require": {
    "php": "^7.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.2",
    "wp-coding-standards/wpcs": "^0.14.0",
    "phpunit/phpunit": "^7.1",
    "johnpbloch/wordpress-core": "^4.9"
  },
  "authors": [
    {
      "name": "John Watkins",
      "email": "john.watkins@colby.edu"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "ColbyComms\\Stories\\": "stories/"
    }
  },
  "scripts": {
    "fix": "phpcbf --extensions=php",
    "lint": "phpcs --extensions=php -s",
    "test": "phpunit"
  }
}
